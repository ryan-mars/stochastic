{
  "configurations": [
    {
      "name": "Attach to SAM CLI",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 9229,
      "localRoot": "${workspaceRoot}/${input:packagePath}",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false
    },
  ],
  "inputs": [
    {
      "id": "packagePath",
      "type": "pickString",
      "options": ["examples/airline/booking", "examples/airline/scheduling"],
      "description": "Path to package to debug"
    }
  ]
}
